public class UC3Compare {
       public static void main(String args[]){
           int x1=5;
           int y1=5;
           int x2=8;
           int y2=4;
           int result = Integer.compare(x1,y1);
           System.out.println("Compare(x1,y1)="+ result );
           int result1 = Integer.compare(x2,y2);
           System.out.println("Compare(x2,y2)="+ result1 );
       }
}
