public class UC2LinesEqual {
    public static void main(String args[]){
        Integer x1=2;
        Integer y1=3;
        Integer x2=4;
        Integer y2=4;
        boolean result = x1.equals(y1);
        System.out.println("Result =" +result);
        boolean result1 = x2.equals(y2);
        System.out.println("Result =" +result1);
    }
}
